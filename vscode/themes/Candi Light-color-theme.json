{
    "name": "Candi Light",
    "type": "light",
    "colors": {
        "focusBorder": "#437085",
        "foreground": "#232A30",
        "widget.shadow": "#E0DED8",
        "selection.background": "#43708540",
        "descriptionForeground": "#5F6469",
        "errorForeground": "#B75B55",
        "terminal.background": "#FBF8F2",
        "terminal.foreground": "#232A30",
        "terminal.ansiBlack": "#1E2226",
        "button.background": "#437085",
        "button.foreground": "#FFFFFF",
        "button.hoverBackground": "#437085",
        "editor.background": "#FBF8F2",
        "editor.foreground": "#232A30",
        "editorLineNumber.foreground": "#83878B",
        "editorLineNumber.activeForeground": "#232A30",
        "editorCursor.foreground": "#437085",
        "editor.selectionBackground": "#43708530",
        "editor.lineHighlightBackground": "#F4F0E7",
        "editorIndentGuide.background": "#E0DED8",
        "editorIndentGuide.activeBackground": "#C7C4BD",
        "sideBar.background": "#F4F0E7",
        "sideBar.foreground": "#5F6469",
        "sideBar.border": "#E0DED8",
        "sideBarTitle.foreground": "#232A30",
        "sideBarSectionHeader.background": "#F4F0E7",
        "sideBarSectionHeader.foreground": "#232A30",
        "sideBarSectionHeader.border": "#E0DED8",
        "activityBar.background": "#FBF8F2",
        "activityBar.foreground": "#232A30",
        "activityBar.inactiveForeground": "#83878B",
        "activityBar.border": "#E0DED8",
        "activityBar.activeBorder": "#437085",
        "editorGroupHeader.tabsBackground": "#F4F0E7",
        "tab.activeBackground": "#FBF8F2",
        "tab.activeForeground": "#232A30",
        "tab.inactiveBackground": "#F4F0E7",
        "tab.inactiveForeground": "#83878B",
        "tab.border": "#E0DED8",
        "tab.activeBorderTop": "#437085",
        "statusBar.background": "#FBF8F2",
        "statusBar.foreground": "#5F6469",
        "statusBar.border": "#E0DED8",
        "titleBar.activeBackground": "#FBF8F2",
        "titleBar.activeForeground": "#232A30",
        "titleBar.inactiveBackground": "#FBF8F2",
        "titleBar.inactiveForeground": "#83878B",
        "titleBar.border": "#E0DED8",
        "list.activeSelectionBackground": "#43708520",
        "list.activeSelectionForeground": "#232A30",
        "list.hoverBackground": "#F4F0E7",
        "list.inactiveSelectionBackground": "#F4F0E7"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#83878B",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#4A754C"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#437085",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "variable.parameter",
                "meta.parameters"
            ],
            "settings": {
                "foreground": "#5F6469"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#1479B0"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#8186D7"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "entity.name.namespace",
                "entity.name.module"
            ],
            "settings": {
                "foreground": "#8186D7"
            }
        },
        {
            "scope": [
                "punctuation.separator",
                "punctuation.terminator",
                "punctuation.accessor"
            ],
            "settings": {
                "foreground": "#5F6469"
            }
        },
        {
            "scope": [
                "markup.heading",
                "entity.name.section"
            ],
            "settings": {
                "foreground": "#437085",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#232A30",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#232A30",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code"
            ],
            "settings": {
                "foreground": "#4A754C"
            }
        },
        {
            "scope": [
                "markup.quote"
            ],
            "settings": {
                "foreground": "#5F6469",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.list"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "markup.underline.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#437085",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "constant.character.escape",
                "constant.other.placeholder"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "string.regexp",
                "constant.regexp"
            ],
            "settings": {
                "foreground": "#CB882E"
            }
        },
        {
            "scope": [
                "support.constant",
                "meta.property-name"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#B0652A",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type.function"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "support.variable",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "support.module",
                "support.node"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "support.type.property-name",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "entity.name.operator"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#5F6469"
            }
        },
        {
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#B75B55"
            }
        },
        {
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#CB882E"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#83878B",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#4A754C"
            }
        },
        {
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#B75B55"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#CB882E"
            }
        },
        {
            "scope": [
                "meta.selector",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "constant.other.color"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.xml"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#83878B"
            }
        },
        {
            "scope": [
                "support.type.vendored.property-name"
            ],
            "settings": {
                "foreground": "#5F6469"
            }
        },
        {
            "scope": [
                "source.css support.type.property-name",
                "source.sass support.type.property-name",
                "source.scss support.type.property-name",
                "source.less support.type.property-name",
                "source.stylus support.type.property-name"
            ],
            "settings": {
                "foreground": "#232A30"
            }
        },
        {
            "scope": [
                "support.constant.mathematical-symbols"
            ],
            "settings": {
                "foreground": "#B0652A"
            }
        },
        {
            "scope": [
                "token.info-token"
            ],
            "settings": {
                "foreground": "#437085"
            }
        },
        {
            "scope": [
                "token.warn-token"
            ],
            "settings": {
                "foreground": "#CB882E"
            }
        },
        {
            "scope": [
                "token.error-token"
            ],
            "settings": {
                "foreground": "#B75B55"
            }
        },
        {
            "scope": [
                "token.debug-token"
            ],
            "settings": {
                "foreground": "#83878B"
            }
        }
    ]
}