        "selection.background": "#4F8FAD40",
        "descriptionForeground": "#A7A49F",
        "errorForeground": "#CE7069",
        "button.background": "#4F8FAD",
        "button.foreground": "#1C2229",
        "button.hoverBackground": "#4F8FAD",
        "editor.background": "#0D1218",
        "editor.foreground": "#E8E4DD",
        "editorLineNumber.foreground": "#7C7A76",
        "editorLineNumber.activeForeground": "#E8E4DD",
        "editorCursor.foreground": "#4F8FAD",
        "editor.selectionBackground": "#4F8FAD30",
        "editor.lineHighlightBackground": "#161B20",
        "editorIndentGuide.background": "#2A2E33",
        "editorIndentGuide.activeBackground": "#43484E",
        "sideBar.background": "#161B20",
        "sideBar.foreground": "#A7A49F",
        "sideBar.border": "#2A2E33",
        "sideBarTitle.foreground": "#E8E4DD",
        "sideBarSectionHeader.background": "#161B20",
        "sideBarSectionHeader.foreground": "#E8E4DD",
        "sideBarSectionHeader.border": "#2A2E33",
        "activityBar.background": "#0D1218",
        "activityBar.foreground": "#E8E4DD",
        "activityBar.inactiveForeground": "#7C7A76",
        "activityBar.border": "#2A2E33",
        "activityBar.activeBorder": "#4F8FAD",
        "editorGroupHeader.tabsBackground": "#161B20",
        "tab.activeBackground": "#0D1218",
        "tab.activeForeground": "#E8E4DD",
        "tab.inactiveBackground": "#161B20",
        "tab.inactiveForeground": "#7C7A76",
        "tab.border": "#2A2E33",
        "tab.activeBorderTop": "#4F8FAD",
        "statusBar.background": "#0D1218",
        "statusBar.foreground": "#A7A49F",
        "statusBar.border": "#2A2E33",
        "titleBar.activeBackground": "#0D1218",
        "titleBar.activeForeground": "#E8E4DD",
        "titleBar.inactiveBackground": "#0D1218",
        "titleBar.inactiveForeground": "#7C7A76",
        "titleBar.border": "#2A2E33",
        "list.activeSelectionBackground": "#4F8FAD20",
        "list.activeSelectionForeground": "#E8E4DD",
        "list.hoverBackground": "#161B20",
        "list.inactiveSelectionBackground": "#161B20"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#7C7A76"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#58905A"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#4F8FAD"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#C77A41"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language"
            ],
            "settings": {
                "foreground": "#E8E4DD"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#4F8FAD"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#C77A41"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#4F8FAD"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#C77A41"
            }
        }
    ]
}